Proceso Ejercicio_3
	Definir l1,l2,l3 Como Real;
	Escribir "Ingresar lado Nro 1";
	Leer l1;
	Escribir "Ingresar lado Nro 2";
	Leer l2;
	Escribir "Ingresar lado Nro 3";
	Leer l3;
	si ((l1+l2)>l3 y (l1+l3)>l2 y (l2+l3)>l1) Entonces
		Escribir "Se puede construir un triangulo";
	SiNo
		Escribir "No se puede construir un triangulo";
	FinSi
	
FinProceso
